<h1>SHOOTINGS SHOW</h1>
<h2>Work in progress, sample images from unsplash.</h2>
<div class="container-fluid">
  <div class="row cards-contain">
    <% @shooting.photos.each do |photo|%>
        <div id = "cat-card" class="card col-xs-12 col-sm-4 col-md-4 col-lg-3 col-xl-2">
          <%= link_to "https://res.cloudinary.com/dovtq7cpo/image/upload/v1586427735/#{url_for(photo.key)}", target: "_blank" do%>
            <%= cl_image_tag photo.key, {class: "card-img-top cat-img"}%>
          <% end %>
        </div>
      <% end %>
  </div>
</div>
